
@{
    ViewBag.Title = "Thêm mới sinh viên";
}

<h2>Thêm mới sinh viên</h2>
<hr />
@using (Html.BeginForm())
{
<div>
    @Html.Label("Họ tên")
    @Html.TextBox("fullName")

</div>
<div>
    @Html.Label("MÃ số")
    @Html.TextBox("MaSV")
</div>
<div>
    @Html.Label("Tài khoản")
    @Html.TextBox("TaiKhoan")
</div>
<div>
    @Html.Label("Mật khẩu")
    @Html.Password("PassWord")
</div>
<button>Submit</button>


}

