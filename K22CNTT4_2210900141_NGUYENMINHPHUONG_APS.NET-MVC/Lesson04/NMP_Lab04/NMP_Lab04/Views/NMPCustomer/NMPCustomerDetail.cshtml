
@{
    ViewBag.Title = "Chi tiết khách hàng";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Chi tiết khách hàng lấy theo cách 1</h2>
<p>Mã số: @Model.CustomerId </p>
